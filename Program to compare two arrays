#include <iostream>
using namespace std;
int main() {
    int n1,n2;
    cout<<"Enter the no. of elements in first array:"<<endl;
    cin>>n1;
    cout<<"Enter the no. of elements in second array:"<<endl;
    cin>>n2;
    int a[n1],b[n2];
    cout<<"Enter the first array:"<<endl;
    for(int i=0;i<n1;i++)
    {
        cin>>a[i];
    }
    cout<<"Enter the second array:"<<endl;
    for(int i=0;i<n2;i++)
    {
        cin>>b[i];
    }
    cout<<"Resultant array"<<endl;
    for(int i=0;i<n1;i++)
    {
        for(int j=0;j<n2;j++)
        {
            if(a[i]==b[j])
            {
                cout<<a[i]<<endl;
                continue;
            }
        }
    }
    return 0;
}
