#include <iostream>
#include<bits/stdc++.h>
#include<string>
using namespace std;
int main() {
    string s,t;
    cin>>s;
    cin>>t;
    sort(s.begin(),s.end());
    sort(t.begin(),t.end());
    if(s==t)
    {
        cout<<"True";
    }
    else
    {
        cout<<"False";
    }
    return 0;
}
