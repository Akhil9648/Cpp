#include <iostream>
#include<map>
using namespace std;
void display(map<int,string> &m1)
{
    for(auto it:m1)
    {
        cout<<it.first<<" "<<it.second<<" ";
    }
}
int main() {
    map<int,string> m;
    m[1]="abc";
    m[5]="def";
    m[3]="Wagon";
    m.insert({4,"Loop"});
    display(m);
    return 0;
}
